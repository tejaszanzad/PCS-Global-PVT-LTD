package java8;

@FunctionalInterface
interface B{
	
	void show();
	
}

public class lambaexpression {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		B obj = new B()
		{
			public void show()
			{
				System.out.println("Anonymous Inner Class");
			}
		};
		obj.show();
	}

}
