package java8;

@FunctionalInterface
interface A{
	
	void show();
	
}

public class lambaexpression {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		A obj = new A()
		{
			public void show()
			{
				System.out.println("Anonymous Inner Class");
			}
		};
		obj.show();
	}

}
