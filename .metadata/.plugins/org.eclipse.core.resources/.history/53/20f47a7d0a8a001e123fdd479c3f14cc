package java8;

@FunctionalInterface
interface A{
	
	void show();
	
}

class B implements A{
	
	public void show() {
		
		System.out.println("Class B Implements Interface A");
	}
}

public class Demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		B obj = new B();
		obj.show();

	}

}
